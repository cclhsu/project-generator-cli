{
  "name": "project-generator-cli",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "copy-template": "node copy-template.js",
    "build": "rimraf dist && nest build && npm run copy-template",
    "clean": "rimraf node_modules dist coverage .nyc_output .eslintcache .cache .jest",
    "pack": "npm run build && mkdir bin && npm pack --pack-destination bin"
  },
  "dependencies": {
    "@nestjs/core": "^10.2.2",
    "@nestjs/platform-express": "^10.2.2",
    "axios": "^1.5.0",
    "csv-parser": "^3.0.0",
    "csv-writer": "^1.6.0",
    "fs": "^0.0.1-security",
    "fs-extra": "^11.1.1",
    "glob": "^10.3.3",
    "js-yaml": "^4.1.0",
    "nest-commander": "^3.11.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.1.16",
    "@nestjs/common": "^10.2.2",
    "@nestjs/schematics": "^10.0.2",
    "@nestjs/swagger": "^7.1.10",
    "@nestjs/testing": "^10.2.2",
    "@types/express": "^4.17.17",
    "@types/fs-extra": "^11.0.1",
    "@types/jest": "^29.5.4",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^20.5.7",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.5.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.6.4",
    "prettier": "^3.0.3",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "main": "dist/cmd.main.js",
  "types": "dist/cmd.main.d.ts",
  "bin": {
    "project-generator": "./dist/cmd.main.js"
  }
}
